{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"esmodules": true
				}
			}
		],
		"next/babel"
	],
	"plugins": [
		[
		  "@babel/plugin-proposal-class-properties",
		  {
			"loose": true
		  }
		],
		[
			"babel-plugin-styled-components",
			{ "ssr": true, "displayName": true, "preprocess": false }
		  ]
	  ]
}
